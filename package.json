  "author": "Aaryanna Simonelli <ashleighsimonelli@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "shleemy": "^1.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{tsx, ts}\"",
    "test": "jest",
    "build": "rm -rf ./dist && tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
